<div class="doctors">
  <ol id='selectable1' class="ui-selectable">
    <% @doctors.each do |doctor| %>
      <li class='doctor ui-widget-content ui-selectee' data-doctorid='<%= doctor.id %>'>
        <%= doctor.doctor_detail.first_name + ' ' + doctor.doctor_detail.last_name %>
      </li>
    <% end %>
  </ol>
</div>
